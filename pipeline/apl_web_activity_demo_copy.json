					"items": {
						"value": "@activity('Get Files').output.childItems",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Move Data From inbound to archive",
							"type": "WebActivity",
							"dependsOn": [],
							"policy": {
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"method": "PUT",
								"headers": {
									"x-ms-copy-source": {
										"value": "@concat('https://souravblobsa1.blob.core.windows.net/anantacontainer/','inbound/', item().name,'?sv=2024-11-04&ss=bfqt&srt=sco&sp=rwdlacupiytfx&se=2025-11-27T14:37:01Z&st=2025-11-27T06:22:01Z&spr=https&sig=vRnUr2Yyrt3xAu5EhLgVGPH82cySKkfE4ggRCFlnEds%3D')",
										"type": "Expression"
