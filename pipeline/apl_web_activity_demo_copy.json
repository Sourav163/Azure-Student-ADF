								"url": {
									"value": "@concat('https://souravblobsa1.blob.core.windows.net/anantacontainer/','archive/',item().name)",
									"type": "Expression"
								},
								"body": "()",
								"authentication": {
									"type": "MSI",
									"resource": "https://storage.azure.com/"
								}
							}
						}
					]
				}
			}
		],
		"folder": {
			"name": "Ananta"
		},
		"annotations": []
	}
