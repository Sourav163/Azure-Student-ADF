{
	"name": "df_00_demo",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_mysql_tb_dyn",
						"type": "DatasetReference"
					},
					"name": "SourceDB"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_blob_csv_folder_dyn",
						"type": "DatasetReference"
					},
					"name": "Jsontgt"
				},
				{
					"dataset": {
						"referenceName": "ds_mysql_tb_dyn",
						"type": "DatasetReference"
					},
					"name": "successtgt"
				}
			],
			"transformations": [
				{
					"name": "skCount"
				},
				{
					"name": "dcBatch"
				},
				{
					"name": "aggregateCollect"
				},
				{
					"name": "derivedFilenms"
				}
			],
			"scriptLines": [
				"parameters{",
				"     run_id as string ('default'),",
				"     batch_nm as integer (1)",
				"}",
				"source(output(",
				"          event_id as integer,",
				"          create_date as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SourceDB",
				"SourceDB keyGenerate(output(rn as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> skCount",
				"skCount derive(batch_dc = floor((rn - 1) / 45) + 1,",
				"          run_id = $run_id,",
				"          model_name = 'try',",
				"          create_date = create_date) ~> dcBatch",
				"derivedFilenms aggregate(groupBy(batch_dc,",
				"          file_nms),",
				"     json = concat('{\"TRYCaseList\":',replace(replace(replace(replace(unescape(toString(collect(event_id)),'json'),'\"]}',']}'),'\"}}\"','\"}}'),'[\"{\"','[{\"'),'\"{\"ad','{\"ad'), '}')) ~> aggregateCollect",
				"dcBatch derive(file_nms = 'try/outbound/TRY_MODEL_'+toString(batch_dc)+'_'+toString($batch_nm)+'.json') ~> derivedFilenms",
				"aggregateCollect sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     rowUrlColumn:'file_nms',",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          json",
				"     ),",
				"     preCommands: [],",
				"     postCommands: []) ~> Jsontgt",
				"dcBatch sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     recreate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> successtgt"
			]
		}
	}
}